<% renderMenu = (items) => %>
  <ul class="sidebar-nav">
    <% for item in items: %>
      <li class="active">
        <h3><a class="sidebar-nav-item" href="<%= item.url %>.html"><%= item.title %></a></h3>
      </li>
    <% end %>
  </ul>
<% end %>
<!-- Run `renderMenu` partial aginst passed `menuItems` meta-data  -->
<%= renderMenu @menuItems %>
